{
  "version": 2,
  "installCommand": "npm install --legacy-peer-deps --ignore-scripts",
  "buildCommand": "echo 'Build complete'",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "maxDuration": 30,
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "routes": [
    {
      "src": "/sse",
      "dest": "/api/index.ts?path=sse"
    },
    {
      "src": "/mcp",
      "dest": "/api/index.ts?path=mcp"
    },
    {
      "src": "/authorize",
      "dest": "/api/index.ts?path=authorize"
    },
    {
      "src": "/callback",
      "dest": "/api/index.ts?path=callback"
    },
    {
      "src": "/token",
      "dest": "/api/index.ts?path=token"
    },
    {
      "src": "/register",
      "dest": "/api/index.ts?path=register"
    },
    {
      "src": "/privacy",
      "dest": "/api/index.ts?path=privacy"
    },
    {
      "src": "/terms",
      "dest": "/api/index.ts?path=terms"
    },
    {
      "src": "/remove",
      "dest": "/api/index.ts?path=remove"
    },
    {
      "src": "/api/?(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}
