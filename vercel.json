{
  "version": 2,
  "buildCommand": "npm run build:vercel",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/?(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/sse",
      "dest": "/api/index.ts?path=sse"
    },
    {
      "src": "/mcp",
      "dest": "/api/index.ts?path=mcp"
    },
    {
      "src": "/authorize",
      "dest": "/api/index.ts?path=authorize"
    },
    {
      "src": "/callback",
      "dest": "/api/index.ts?path=callback"
    },
    {
      "src": "/token",
      "dest": "/api/index.ts?path=token"
    },
    {
      "src": "/register",
      "dest": "/api/index.ts?path=register"
    },
    {
      "src": "/privacy",
      "dest": "/api/index.ts?path=privacy"
    },
    {
      "src": "/terms",
      "dest": "/api/index.ts?path=terms"
    },
    {
      "src": "/remove",
      "dest": "/api/index.ts?path=remove"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ],
  "env": {
    "GOOGLE_CLIENT_ID": "@google_client_id",
    "GOOGLE_CLIENT_SECRET": "@google_client_secret",
    "COOKIE_ENCRYPTION_KEY": "@cookie_encryption_key",
    "JWT_SECRET": "@jwt_secret"
  }
}
